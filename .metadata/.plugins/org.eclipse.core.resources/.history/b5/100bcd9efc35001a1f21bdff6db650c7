package thread;
class MyThread extends Thread
{
	public void run()
	{
		super.run();
		for(int i=0;i<=10;i++){
			System.out.println("hi");
		}
	}
}
public class ThreadExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
MyThread t=new MyThread();
t.start();
	}

}
